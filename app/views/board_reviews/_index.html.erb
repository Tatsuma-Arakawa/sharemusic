<% @boardreviews.each do |boardreview| %>
  <%= link_to user_path(boardreview.user.id) do %>
    <div>
      <p><%= boardreview.user.username %></p>
      <%= image_tag "#{boardreview.user.image}", :size =>'30x30' %>
    </div>
  <% end %>
  <div>
    <p><%= boardreview.content %></p>
  </div>
<% end %> 